@model eProject3.Models.Entities.AboutUs

<link rel="stylesheet" href="~/css/AboutUs4.css">

<div class="mainHead">
    <div class="tilteLeft">
        <h1>DASHBOARD</h1>
        <p style="color: gray; padding-left: 5px;">Version 2.0</p>
    </div>
    <div class="tilteRight">
        <span>Home ></span>
        <span style="padding-left: 4px;">Dashboard</span>
    </div>
</div>
<div class="mainBody">
    <h1>Career with us</h1>
    <div class="content">
        <ul>
            @foreach (var child in Model.AboutUsChilds)
            {
                <li class="list-content-parent">@child.title</li>
                <ul>
                    <li class="list-content-child" style="list-style: inside;">
                        @child.content
                    </li>
                </ul>
            }
            
        </ul>
    </div>
</div>
<div class="btn">
    <button class="actionBtn editBtn"><i class="fas fa-edit"></i>Edit</button>
</div>